<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.apicloud.pkg.sdk"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="16"
        android:targetSdkVersion="20" />

    <supports-screens
        android:anyDensity="true"
        android:largeScreens="true"
        android:normalScreens="true"
        android:resizeable="true"
        android:smallScreens="true"
        android:xlargeScreens="true" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <meta-data
        android:name="android.support.VERSION"
        android:value="25.3.1" />

    <application
        android:name="com.apicloud.moduleDemo.base.BaseApplication"
        android:allowBackup="false"
        android:debuggable="true"
        android:hardwareAccelerated="true"
        android:icon="@drawable/uz_icon"
        android:label="@string/app_name"
        android:largeHeap="true"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="uz_version"
            android:value="1.2.0" />

        <activity
            android:name="com.uzmap.pkg.EntranceActivity"
            android:configChanges="orientation|locale|keyboardHidden|keyboard|screenLayout|screenSize|smallestScreenSize"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.apicloud.moduleDemo.MainActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" /> <!-- 我的消息 -->
        <activity
            android:name="com.apicloud.moduleDemo.MyMessageActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 我的悬赏 -->
        <activity
            android:name="com.apicloud.moduleDemo.MyRewardActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 我的悬赏详情 -->
        <!-- 创业加盟 -->
        <activity
            android:name="com.apicloud.moduleDemo.MyJoinInActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 赚钱大厅 -->
        <activity
            android:name="com.apicloud.moduleDemo.MoneyMakingHallActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 资格商家 -->
        <activity
            android:name="com.apicloud.moduleDemo.BusinessActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 咨询与建议 -->
        <activity
            android:name="com.apicloud.moduleDemo.InformationActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 发布活动  装修量房 -->
        <activity
            android:name="com.apicloud.moduleDemo.ReleaseRenovationActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 发布活动  买建材 -->
        <activity
            android:name="com.apicloud.moduleDemo.ReleaseBuildingActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 发布活动  减肥 -->
        <activity
            android:name="com.apicloud.moduleDemo.ReleaseReduceWeightActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 发布活动  戒烟 -->
        <activity
            android:name="com.apicloud.moduleDemo.ReleaseSmokingActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 发布活动  戒酒 -->
        <activity
            android:name="com.apicloud.moduleDemo.ReleaseDrinkingActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 发布活动  戒赌 -->
        <activity
            android:name="com.apicloud.moduleDemo.ReleaseGiveUpGamblingActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 活动  详情 -->
        <activity
            android:name="com.apicloud.moduleDemo.DetailsActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 评论 活动  详情 评论 -->
        <activity
            android:name="com.apicloud.moduleDemo.CommentActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 推荐客户 -->
        <activity
            android:name="com.apicloud.moduleDemo.RecommentActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 支付保证金 -->
        <activity
            android:name="com.apicloud.moduleDemo.PaymentActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 订单支付 -->
        <activity
            android:name="com.apicloud.moduleDemo.OrderPaymentActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 创业列表 -->
        <activity
            android:name="com.apicloud.moduleDemo.EntrepreneurshipActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 设计师/项目经理 详情 -->
        <activity
            android:name="com.apicloud.moduleDemo.EntrepreneurshipDetailsActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 申请创业 -->
        <activity
            android:name="com.apicloud.moduleDemo.ApplyEntrepreneurshipActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- 找公司 -->
        <activity
            android:name="com.apicloud.moduleDemo.FindCompanyActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" /> <!-- RxGalleryFinal -->
        <activity
            android:name="cn.finalteam.rxgalleryfinal.ui.activity.MediaActivity"
            android:exported="true"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />
        <activity
            android:name="com.yalantis.ucrop.UCropActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar" />

        <meta-data
            android:name="android.support.VERSION"
            android:value="26.1.0" />

        <service
            android:name="com.squareup.leakcanary.internal.HeapAnalyzerService"
            android:enabled="false"
            android:process=":leakcanary" />
        <service
            android:name="com.squareup.leakcanary.DisplayLeakService"
            android:enabled="false"
            android:process=":leakcanary" />

        <activity
            android:name="com.squareup.leakcanary.internal.DisplayLeakActivity"
            android:enabled="false"
            android:icon="@drawable/leak_canary_icon"
            android:label="@string/leak_canary_display_activity_label"
            android:process=":leakcanary"
            android:taskAffinity="com.squareup.leakcanary.com.apicloud.pkg.sdk"
            android:theme="@style/leak_canary_LeakCanary.Base" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.squareup.leakcanary.internal.RequestStoragePermissionActivity"
            android:enabled="false"
            android:excludeFromRecents="true"
            android:icon="@drawable/leak_canary_icon"
            android:label="@string/leak_canary_storage_permission_activity_label"
            android:process=":leakcanary"
            android:taskAffinity="com.squareup.leakcanary.com.apicloud.pkg.sdk"
            android:theme="@style/leak_canary_Theme.Transparent" />

        <meta-data
            android:name="android.arch.lifecycle.VERSION"
            android:value="27.0.0-SNAPSHOT" />
    </application>

</manifest>